{
  "swagger": "2.0",
  "info": {
    "description": "This is an API of the Outline Server that allows a Strapi web app to receive/modify/remove the Outline VPN Keys on Outline Server containers which are handled by [Outline Server API](https://github.com/Jigsaw-Code/outline-server).",
    "version": "1.0.0",
    "title": "Outline Server Management API",
    "termsOfService": "https://hypervpn.com/terms",
    "contact": {
      "email": "support@hypervpn.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "host": "localhost:8080",
  "basePath": "/api",
  "tags": [
    {
      "name": "keys",
      "description": "Outline VPN Keys",
      "externalDocs": {
        "description": "Find out more about our store",
        "url": "http://swagger.io"
      }
    }
  ],
  "schemes": [
    "https",
    "http"
  ],
  "paths": {
    "/keys/all": {
      "get": {
        "tags": [
          "keys"
        ],
        "summary": "Get all the keys out there",
        "description": "",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Key"
              }
            }
          },
          "500": {
            "description": "Internal Error"
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    },
    "/keys/new": {
      "post": {
        "tags": [
          "keys"
        ],
        "summary": "Get a new key based on the plan and traffic needs and stuff.",
        "description": "",
        "produces": [
          "application/json"
        ],
        "parameters": [],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/Key"
            }
          },
          "400": {
            "description": "bad user input",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          },
          "500": {
            "description": "Internal Error",
            "schema": {
              "$ref": "#/definitions/Error"
            }
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    },
    "/keys/delete/{key}": {
      "delete": {
        "tags": [
          "keys"
        ],
        "summary": "Delete a key given its key value",
        "description": "",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "key",
            "in": "path",
            "description": "value of VPN key to remove",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "key has been deleted"
          }
        },
        "security": [
          {
            "api_key": []
          }
        ]
      }
    }
  },
  "securityDefinitions": {
    "api_key": {
      "type": "apiKey",
      "name": "api_key",
      "in": "header"
    }
  },
  "definitions": {
    "Key": {
      "type": "object",
      "properties": {
        "keyValue": {
          "type": "string",
          "description": "VPN Key Value | CONFIDENTIAL SENSITIVE DATA"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "Error": {
      "type": "object",
      "properties": {
        "httpStatusCode": {
          "type": "integer",
          "format": "int32"
        },
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "msg": {
          "type": "string"
        },
        "userError": {
          "type": "string"
        },
        "detail": {
          "type": "string"
        }
      }
    },
    "Tag": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "name": {
          "type": "string"
        }
      },
      "xml": {
        "name": "Tag"
      }
    },
    "ApiResponse": {
      "type": "object",
      "properties": {
        "status": {
          "type": "boolean"
        },
        "type": {
          "type": "string"
        },
        "message": {
          "type": "string"
        }
      }
    }
  }
