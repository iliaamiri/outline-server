{
  "name": "osm-api",
  "version": "1.0.0",
  "description": "Outline Server Management API - This is an API that the Strapi web app will communicate with.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start:prod": "npm run build && node ./dist/server.js",
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm ./server.ts",
    "dev": "nodemon --config nodemon.json server.ts",
    "build": "npx tsc"
  },
  "keywords": [
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/swagger-ui-express": "^4.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^5.0.0-beta.1",
    "express-session": "^1.17.2",
    "swagger-ui-dist": "^4.10.3",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@types/express-session": "^1.17.4",
    "@types/node": "^17.0.31",
    "@types/swagger-ui-dist": "^3.30.1",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
